<Window x:Class="HashtableProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashtableProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Keytxt" HorizontalAlignment="Left" Margin="210,137,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="389" Height="22"/>
        <TextBox x:Name="Valuetxt" HorizontalAlignment="Left" Margin="210,181,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="389" Height="22"/>
        <Button x:Name="AddBtn" HorizontalAlignment="Left" Margin="210,238,0,0" VerticalAlignment="Top" Height="38" Width="120" Click="AddBtn_Click" Content="Add"/>
        <Button x:Name="RemoveBtn" HorizontalAlignment="Left" Margin="344,238,0,0" VerticalAlignment="Top" Height="38" Width="120" Click="RemoveBtn_Click" Content="Remove"/>
        <Button x:Name="ShowAllBtn" HorizontalAlignment="Left" Margin="479,238,0,0" VerticalAlignment="Top" Height="38" Width="120" Click="ShowAllBtn_Click" Content="Show All"/>

    </Grid>
</Window>
